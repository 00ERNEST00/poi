{
  "name": "vbuild",
  "version": "6.18.1",
  "description": "Refined webpack development experience for Vue.js",
  "repository": {
    "url": "egoist/vbuild",
    "type": "git"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "template"
  ],
  "engines": {
    "node": ">= 6"
  },
  "bin": "bin/vbuild",
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "xo",
    "toc": "markdown-toc -i README.md && markdown-toc -i docs/recipes/deployment.md && markdown-toc -i docs/troubleshooting.md",
    "precommit": "npm test"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "husky": "^0.13.1",
    "jest-cli": "^19.0.2",
    "markdown-toc": "^1.1.0",
    "update-pkg": "^1.1.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "examples/**",
      "template/**"
    ],
    "envs": [
      "jest"
    ],
    "rules": {
      "guard-for-in": 0,
      "import/no-dynamic-require": 0,
      "max-nested-callbacks": [
        "error",
        6
      ]
    }
  },
  "dependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-vue-app": "^0.4.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "co": "^4.6.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cosmiconfig": "^2.1.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.2",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.1",
    "eslint-config-vue-app": "^1.1.0",
    "eslint-loader": "^1.6.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "installed-by-yarn-globally": "^0.1.2",
    "kopy": "^5.0.2",
    "lodash.merge": "^4.6.0",
    "meow": "^3.7.0",
    "mz": "^2.6.0",
    "node-notifier": "^5.0.2",
    "opn": "^4.0.2",
    "post-compile-webpack-plugin": "^0.1.1",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.2",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.6",
    "strip-ansi": "^3.0.1",
    "tildify": "^1.2.0",
    "update-notifier": "^2.1.0",
    "url-loader": "^0.5.8",
    "vue": "^2.2.0",
    "vue-loader": "^11.1.3",
    "vue-template-compiler": "^2.2.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^3.0.0",
    "webpack-node-externals": "^1.5.4",
    "yarn-install": "^0.2.1"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "stage-2"
        ],
        "plugins": [
          "transform-runtime"
        ]
      }
    }
  }
}
