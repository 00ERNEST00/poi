{
  "name": "vbuild",
  "version": "6.0.0",
  "description": "Develop Vue app with zero-config.",
  "repository": {
    "url": "egoist/vbuild",
    "type": "git"
  },
  "files": [
    "bin",
    "lib",
    "template"
  ],
  "engines": {
    "node": ">= 7"
  },
  "bin": "bin/vbuild",
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "xo --fix"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "jest-cli": "^18.1.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "examples/**",
      "template/**"
    ],
    "envs": [
      "jest"
    ],
    "rules": {
      "guard-for-in": 0,
      "import/no-dynamic-require": 0,
      "unicorn/no-process-exit": 0
    }
  },
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-preset-vue-app": "^0.4.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "installed-by-yarn-globally": "^0.1.2",
    "kopy": "^5.0.2",
    "opn": "^4.0.2",
    "post-compile-webpack-plugin": "^0.1.1",
    "postcss-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.6",
    "tildify": "^1.2.0",
    "update-notifier": "^2.0.0",
    "url-loader": "^0.5.7",
    "vue": "^2.1.10",
    "vue-loader": "^11.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  }
}
