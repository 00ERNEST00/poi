{
  "name": "vbuild",
  "version": "6.9.2",
  "description": "Refined webpack development experience for Vue.js",
  "repository": {
    "url": "egoist/vbuild",
    "type": "git"
  },
  "files": [
    "bin",
    "lib",
    "template"
  ],
  "engines": {
    "node": ">= 6"
  },
  "bin": "bin/vbuild",
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "xo --fix",
    "toc": "markdown-toc -i README.md",
    "precommit": "npm test"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "husky": "^0.13.1",
    "jest-cli": "^18.1.0",
    "markdown-toc": "^1.1.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "examples/**",
      "template/**"
    ],
    "envs": [
      "jest"
    ],
    "rules": {
      "guard-for-in": 0,
      "import/no-dynamic-require": 0,
      "unicorn/no-process-exit": 0
    }
  },
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-preset-vue-app": "^0.4.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.15.0",
    "eslint-config-vue-app": "^1.0.0",
    "eslint-loader": "^1.6.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "installed-by-yarn-globally": "^0.1.2",
    "interpolate-html-plugin": "^1.0.1",
    "kopy": "^5.0.2",
    "meow": "^3.7.0",
    "opn": "^4.0.2",
    "post-compile-webpack-plugin": "^0.1.1",
    "postcss-loader": "^1.3.0",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.6",
    "tildify": "^1.2.0",
    "update-notifier": "^2.0.0",
    "url-loader": "^0.5.7",
    "vue": "^2.1.10",
    "vue-loader": "^11.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1",
    "yarn-install": "^0.2.1"
  }
}